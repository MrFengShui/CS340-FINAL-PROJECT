    <td class="vend-info-row id">{{vendorid}}</td>
    <td class="vend-info-row name">{{vendorname}}</td>
    <td class="vend-info-row address">{{vendoraddress}}</td>
    <td class="vend-info-row phone">{{vendorphone}}</td>
    <td class="vend-info-row email">{{vendoremail}}</td>
    <td class="book-info-row type">{{booktype}}</td>
