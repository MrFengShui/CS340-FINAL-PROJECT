<tr>
    <td class="vend-info-row select">
        {{#if checked}}
            <input type="checkbox" name="" value="" checked>
        {{else}}
            <input type="checkbox" name="" value="">
        {{/if}}
    </td>
    <td class="vend-info-row id">{{vendorid}}</td>
    <td class="vend-info-row name">{{vendorname}}</td>
    <td class="vend-info-row address">{{vendoraddress}}</td>
    <td class="vend-info-row phone">{{vendorphone}}</td>
    <td class="vend-info-row email">{{vendoremail}}</td>
    <th class="store-info-row id">{{repoid}}</th>
    <td class="book-info-row type">{{booktype}}</td>
    <td class="vend-info-row control">
        <button type="button" name="change-info-button" class="staff-vend-edit-button" onclick="todoVendModifyEdit(this)"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button>
        <button type="button" name="remove-info-button" class="staff-vend-delete-button" onclick="todoDeleteSingleVendInfo(this)"><i class="fa fa-trash-o" aria-hidden="true"></i></button>
    </td>
</tr>
